program Contatos;

uses
  Vcl.Forms,
  U_Cadastro in '..\Forms\U_Cadastro.pas' {Frm_cadastro},
  U_Pesquisa in '..\Forms\U_Pesquisa.pas' {Form_Pesquisa};

